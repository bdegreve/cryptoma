{
  "name": "cryptoma",
  "version": "2.0.0",
  "description": "crypto tool",
  "main": "webpack.config.js",
  "scripts": {
    "start": "npm run dev",
    "test": "npm run test:lint && npm run test:type && npm run test:unit",
    "test:lint": "standard",
    "test:type": "flow check",
    "test:unit": "jest",
    "dist": "cross-env NODE_ENV=production webpack --progress --colors",
    "translations": "node scripts/build-translations.js",
    "dev": "node dev-server.js"
  },
  "author": "Bram de Greve <bram.degreve@bramz.net>",
  "license": "ISC",
  "private": true,
  "standard": {
    "parser": "babel-eslint"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/app/"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "testURL": "http://localhost/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bdegreve/cryptoma.git"
  },
  "dependencies": {
    "autoprefixer": "^8.6.5",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-react-intl": "^2.4.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "^3.4.0",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "esrever": "~0.2.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "flow-bin": "~0.69.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "postcss-loader": "^2.1.6",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.6.3",
    "react-intl": "^2.7.2",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "redux": "^3.7.2",
    "redux-persist": "^5.10.0",
    "standard": "^11.0.1",
    "standard-loader": "^6.0.1",
    "style-loader": "~0.20.3",
    "webpack": "^3.12.0"
  },
  "devDependencies": {
    "babel-jest": "^22.4.4",
    "connect-history-api-fallback": "^1.5.0",
    "express": "^4.16.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.4",
    "react-intl-translations-manager": "^5.0.3",
    "webpack-dev-middleware": "^1.12.2"
  }

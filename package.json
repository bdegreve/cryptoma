    "connect-history-api-fallback": "^1.3.0",
    "express": "^4.15.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "react-intl-translations-manager": "^5.0.0",
    "webpack-dev-middleware": "^1.10.2"

    "html-webpack-plugin": "^2.28.0",
    "less": "^2.7.2",
    "less-loader": "^3.0.0",
    "postcss-loader": "^2.0.5",
    "react": "^16.0.0",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^16.0.0",
    "react-intl": "^2.3.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",

  "repository": {
    "type": "git",
    "url": "https://github.com/bdegreve/cryptoma.git"
  },
  "dependencies": {
    "autoprefixer": "^7.1.1",
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "3.3.7",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.1",
    "esrever": "^0.2.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
